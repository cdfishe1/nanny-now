
<div class="row">
  <div class="col-12">
    <div class="card card-margin">
      <div class="card-body">
        <div class="row search-body">
          <div class="col-lg-12">
            <div class="search-result">
              <div class="result-header">
                <div class="result-body">
                  <div class="table-responsive">
                    <table class="table class-1">
                      <tbody>
                        <tr>
                          <td>
                            <div class="class-1-job-emp-img">
                              <img
                                id="nannyImage"
                                class="img-fluid"
                                src="/assets/{{filename}}"
                                alt="personalPhoto"
                              />
                            </div>
                          </td>
                          <td>
                            <div class="class-1-name-skills">
                              <p>Name: {{username}}</p>
                              <p class="m-0 languages">Languages:
                                {{languages}}</p>
                            </div>
                          </td>
                          <td>
                            <div class="class-1-availability-info">
                              <p class="type m-0">{{gender}}</p>
                              <p class="text-muted m-0 location">Location:
                                {{location}}</p>
                            </div>
                          </td>
                          <td>
                            <div class="class-1-job-salary">Experience:
                              {{years_experience}}
                              year(s)</div>
                          </td>

                          <td>
                            <div class="class-1-job-category bg-soft-base">
                              <span>Education Level: {{education}}</span>
                            </div>
                          </td>
                        </tr>

                      </tbody>

                    </table>

                  </div>
                </div>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>